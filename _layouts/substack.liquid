---
layout: default
---
<div class="post">
  <header class="post-header">
    <h1 class="post-title">{{ page.title }}</h1>
    <p class="post-description">{{ page.description }}</p>
  </header>

  <article>
    <div class="substack-embed">
      <iframe src="https://substack.com/embed/@cowthattypes" width="100%" height="800" style="border:none; background:white;" frameborder="0" scrolling="no"></iframe>
    </div>
    
    <div class="substack-description">
      {{ content }}
    </div>
  </article>
</div>
